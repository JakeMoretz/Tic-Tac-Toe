{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
   
    "rules": {
        "no-console":"off",
        "no-use-before-define":"off",
        "no-plusplus":"off",
        "consistent-return":"off"
    }
}
